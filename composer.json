{
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "config/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "vlucas/phpdotenv": "^5.5",
        "psr/container": "^2.0",
        "illuminate/container": "^10.1",
        "illuminate/view": "^10.1",
        "illuminate/database": "^10.1",
        "illuminate/events": "^10.1",
        "symfony/console": "^6.2"
    },
    "config": {
        "optimize-autoloader": true
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    }
}
